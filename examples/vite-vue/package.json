{
  "name": "@cdlab996/eslint-vite-vue",
  "type": "module",
  "version": "0.0.12",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint ./src/**/*.{vue,ts,tsx}",
    "lint:fix": "eslint ./src/**/*.{vue,ts,tsx} --fix",
    "prettier": "prettier --check \"./src/**/*.{vue,ts,tsx}\" \"./vite.config.ts\"",
    "prettier:write": "prettier --check --write \"./src/**/*.{vue,ts,tsx}\" \"./vite.config.ts\""
  },
  "dependencies": {
    "vue": "^3.4.27"
  },
  "devDependencies": {
    "@cdlab996/eslint-config-vue": "workspace:*",
    "@cdlab996/prettier-config": "workspace:*",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "eslint": "8.57.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vue-tsc": "^2.0.6"
  }
}
